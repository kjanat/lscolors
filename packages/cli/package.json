	"description": "Convert between macOS/BSD LSCOLORS and GNU LS_COLORS terminal color formats",
	"keywords": [
		"lscolors",
		"ls_colors",
		"terminal",
		"ansi",
		"colors",
		"bsd",
		"gnu",
		"macos",
		"converter"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/kjanat/lscolors",
		"directory": "packages/cli"
	},
	"license": "MIT",
	"type": "module",
	"bin": {
